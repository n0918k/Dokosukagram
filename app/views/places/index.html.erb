<%= render "shared/header"%>
<%= form_with model:@place,url: "/places",method: :create,local: true  do|f|%>
<%= f.file_field :image%>
<%= f.text_field :name%>
<%= f.submit "登録する" %>
<% end %>
<% @places.each do |place|%>
<%= place.name%>
<%= link_to place_path(place.id)do%>
<%= image_tag place.image%>
<% end%>
<% end %>